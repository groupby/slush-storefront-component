    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "condition-circle": "^1.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.0.0",
    "eslint-config-tough": "^0.4.0",
