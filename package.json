  "homepage": "https://github.com/groupby/slush-storefront-component",
  "author": [{
    "name": "Ben Teichman",
    "email": "ben.teichman@groupbyinc.com"
  }],
  "repository": {
    "type": "git",
    "url": "https://github.com/groupby/slush-storefront-component.git"
  },
